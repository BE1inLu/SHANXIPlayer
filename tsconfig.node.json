{
    "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
    "include": [
        "electron.vite.config.*", 
        "src/main/*",
        "src/main/config/*",
        "src/main/service/*", 
        "src/preload/*",
        "src/main/util/*"
        ],
    "compilerOptions": {
        "composite": true,
        "types": ["electron-vite/node"],
        "baseUrl": ".",
        "paths": {
            "@main/*":["src/main/*"]
        }
  }
}
